{
  "name": "cobalt-workflow-sdk",
  "version": "1.0.0",
  "main": "dist/cobalt-workflow-sdk.cjs.js",
  "module": "dist/cobalt-workflow-sdk.esm.js",
  "exports": {
    ".": {
      "import": "./dist/cobalt-workflow-sdk.esm.js",
      "require": "./dist/cobalt-workflow-sdk.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "yarn clean && tsc --declaration --emitDeclarationOnly --outDir dist",
    "build": "microbundle --no-sourcemap",
    "sample": "ts-node test/sample_workflow_example.ts",
    "build-publish": "bash scripts/build-publish.sh"
  },
  "repository": "https://github.com/gocobalt/workflow_as_code",
  "author": "Dhananjay-JSR <ajaysenday@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
